#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/PersianBlocker/PersianBlocker/node_modules/.pnpm/@adguard+aglint@2.0.9/node_modules/@adguard/aglint/dist/node_modules:/home/runner/work/PersianBlocker/PersianBlocker/node_modules/.pnpm/@adguard+aglint@2.0.9/node_modules/@adguard/aglint/node_modules:/home/runner/work/PersianBlocker/PersianBlocker/node_modules/.pnpm/@adguard+aglint@2.0.9/node_modules/@adguard/node_modules:/home/runner/work/PersianBlocker/PersianBlocker/node_modules/.pnpm/@adguard+aglint@2.0.9/node_modules:/home/runner/work/PersianBlocker/PersianBlocker/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/PersianBlocker/PersianBlocker/node_modules/.pnpm/@adguard+aglint@2.0.9/node_modules/@adguard/aglint/dist/node_modules:/home/runner/work/PersianBlocker/PersianBlocker/node_modules/.pnpm/@adguard+aglint@2.0.9/node_modules/@adguard/aglint/node_modules:/home/runner/work/PersianBlocker/PersianBlocker/node_modules/.pnpm/@adguard+aglint@2.0.9/node_modules/@adguard/node_modules:/home/runner/work/PersianBlocker/PersianBlocker/node_modules/.pnpm/@adguard+aglint@2.0.9/node_modules:/home/runner/work/PersianBlocker/PersianBlocker/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@adguard/aglint/dist/aglint.cli.js" "$@"
else
  exec node  "$basedir/../@adguard/aglint/dist/aglint.cli.js" "$@"
fi
