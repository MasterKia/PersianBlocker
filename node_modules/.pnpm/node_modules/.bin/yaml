#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/PersianBlocker/PersianBlocker/node_modules/.pnpm/yaml@2.4.5/node_modules/yaml/node_modules:/home/runner/work/PersianBlocker/PersianBlocker/node_modules/.pnpm/yaml@2.4.5/node_modules:/home/runner/work/PersianBlocker/PersianBlocker/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/PersianBlocker/PersianBlocker/node_modules/.pnpm/yaml@2.4.5/node_modules/yaml/node_modules:/home/runner/work/PersianBlocker/PersianBlocker/node_modules/.pnpm/yaml@2.4.5/node_modules:/home/runner/work/PersianBlocker/PersianBlocker/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../yaml/bin.mjs" "$@"
else
  exec node  "$basedir/../yaml/bin.mjs" "$@"
fi
